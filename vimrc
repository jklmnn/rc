set nocompatible

syntax on

set wildmenu
set showcmd
set encoding=utf-8
set autoindent
set number
set showmatch
set hlsearch
set ignorecase
set smartcase
set backspace=indent,eol,start
set pastetoggle=<F11>
set shiftwidth=4
set softtabstop=4
set expandtab
set cindent
set splitright

map Y y$

let g:ycm_enable_diagnostic_highlighting = 0
let g:ycm_error_symbol = 'e:'
let g:ycm_warning_symbol = 'w:'
